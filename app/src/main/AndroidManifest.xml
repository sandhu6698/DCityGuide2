<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.example.lenovo.cityguide">

    <uses-permission android:name="android.permission.INTERNET" />

    <application
        android:allowBackup="true"
        android:icon="@mipmap/ic_launcher"
        android:label="@string/app_name"
        android:roundIcon="@mipmap/ic_launcher_round"
        android:supportsRtl="true"
        android:theme="@style/AppTheme"
        android:usesCleartextTraffic="true">
        <activity
            android:name=".MainActivity"
            android:label="Dublin Guide"
            android:theme="@style/AppTheme.NoActionBar">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity
            android:name=".HotelsActivity"
            android:label="Hotel&apos;s" />
        <activity
            android:name=".RestaurantActivity"
            android:label="@string/title_activity_restaurant"
            android:theme="@style/AppTheme.NoActionBar" />
        <activity
            android:name=".CafeActivity"
            android:label="@string/title_activity_cafe"
            android:theme="@style/AppTheme.NoActionBar" />
        <activity
            android:name=".BeachActivity"
            android:label="@string/title_activity_beach"
            android:theme="@style/AppTheme.NoActionBar"></activity>
    </application>

</manifest>